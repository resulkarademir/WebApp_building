<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FreeBuild</title>

    <link rel="shortcut icon" href="~/freebuild-html/assets/img/favicon.png">

    <link rel="stylesheet" href="~/freebuild-html/assets/css/fontawesome.min.css">
    <link rel="stylesheet" href="~/freebuild-html/assets/css/animate.css">
    <link rel="stylesheet" href="~/freebuild-html/assets/css/magnific-popup.css">
    <link rel="stylesheet" href="~/freebuild-html/assets/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/freebuild-html/assets/css/owl.theme.css">
    <link rel="stylesheet" href="~/freebuild-html/assets/css/meanmenu.min.css">
    <link rel="stylesheet" href="~/freebuild-html/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/freebuild-html/assets/css/default.css">

    <link rel="stylesheet" href="~/freebuild-html/style.css">

    <link rel="stylesheet" href="~/freebuild-html/assets/css/responsive.css">
</head>
<body>

    <div class="main-wrapper">
        <header class="header-one">
            <div class="top-header pl-75 pr-75 d-none d-md-block">
                <div class="container-fluid">
                    <div class="row d-flex align-items-center">
                        <div class="col-lg-8 col-md-9 top-bar-left">
                            <span><i class="fas fa-phone"></i> +123 365 585</span>
                            <span><i class="fas fa-phone"></i> hammer@gmailcom</span>
                            <span><i class="fas fa-clock"></i> SUN-MON, 9:45AM </span>
                        </div>
                        <div class="col-lg-4 col-md-3 text-md-right">
                            <div class="social-profile">
                                <ul>
                                    <li><a href="#"><i class="fab fa-facebook-f"></i></a></li>
                                    <li><a href="#"><i class="fab fa-twitter"></i></a></li>
                                    <li><a href="#"><i class="fab fa-linkedin-in"></i></a></li>
                                    <li><a href="#"><i class="fab fa-google-plus-g"></i></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="main-menu pl-75 pr-75 pt-30 pb-30" id="sticky-nav">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-3 col-lg-2 d-flex">
                            <a class="navbar-brand logo" asp-controller="Home" asp-action="Index">
                                <img src="~/freebuild-html/assets/img/logo.png" class="logo-display" alt="shipo">
                            </a>
                        </div>
                        <div class="col-lg-10 col-md-9  d-none d-lg-block text-lg-right">
                            <nav id="responsive-menu" class="menu-style-one">
                                <ul class="menu-items">
                                    <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                                    <li><a href="#whatwedo">About</a></li>
                                    <li><a href="#service">Services</a></li>
                                    <li><a href="#portfolio">Projects</a></li>
                                    <li><a href="#news">News</a></li>
                                    <li><a href="#contact">Contact</a></li>
                                </ul>
                            </nav>
                        </div>
                        <div class="col-12">
                            <div class="responsive-menu"></div>
                        </div>
                    </div>
                </div>
            </div>
        </header>

        @RenderBody()
        <footer class="footer-section pt-150 pb-30">
            <div class="container pb-80">
                <div class="row">
                    <div class="col-lg-3 col-md-6">
                        <div class="single-footer-widget">
                            <h3>About Us</h3>
                            <p>Lorem ipsum dolor sit amet, conse ctetur adipisicing elit.</p>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="single-footer-widget">
                            <h3>Quick Links</h3>
                            <ul>
                                <li><a href="#">Privacy</a></li>
                                <li><a href="#">Contact us</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="footer-bootm pt-30">
                <div class="container">
                    <div class="row d-flex align-self-center">
                        <div class="col-lg-6 col-md-6">
                            <span>Copyright ©2025 FreeBuild. All Rights Reserved</span>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    <script src="~/freebuild-html/assets/js/jquery-1.12.4.min.js"></script>
    <script src="~/freebuild-html/assets/js/bootstrap.min.js"></script>
    <script src="~/freebuild-html/assets/js/active.js"></script>
    <script src="~/freebuild-html/assets/js/ajax-mail.js"></script>
    <script src="~/freebuild-html/assets/js/counterup.min.js"></script>
    <script src="~/freebuild-html/assets/js/equalheights.min.js"></script>
    <script src="~/freebuild-html/assets/js/imageload.min.js"></script>
    <script src="~/freebuild-html/assets/js/isotope.pkgd.min.js"></script>
    <script src="~/freebuild-html/assets/js/jquery.easing.1.3.js"></script>
    <script src="~/freebuild-html/assets/js/magnific-popup.min.js"></script>
    <script src="~/freebuild-html/assets/js/meanmenu.min.js"></script>
    <script src="~/freebuild-html/assets/js/owl.carousel.min.js"></script>
    <script src="~/freebuild-html/assets/js/popper.min.js"></script>
    <script src="~/freebuild-html/assets/js/scrollup.min.js"></script>
    <script src="~/freebuild-html/assets/js/sticky.js"></script>
    <script src="~/freebuild-html/assets/js/waypoint.js"></script>
    <script src="~/freebuild-html/assets/js/wow.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>