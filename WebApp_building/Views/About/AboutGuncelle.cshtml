@model WebApp_building.DAL.Entities.Hakkimizda
@{
    ViewData["Title"] = "AboutUpdate";
    Layout = "~/Views/Layout/Index.cshtml";
}
<div class="main-panel">
    <div class="content">
        <div class="container-fluid">
            <h4 class="page-title">About</h4>
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <div class="card-title">About Update</div>
                        </div>
                        <div class="card-body">

                            <form method="post">
                                <input type="hidden" asp-for="id" />
                                <label>About Title</label>
                                <input type="text" asp-for="Title" class="form-control" />
                                <br />
                                <label>About SubDescription</label>
                                <input type="text" asp-for="Icon" class="form-control" />
                                <br />
                          
                                <button class="btn btn-primary"> About Update</button>
                            </form>


                        </div>
                    </div>

                </div>

            </div>
        </div>
    </div>
</div>

