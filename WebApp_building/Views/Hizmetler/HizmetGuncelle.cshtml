@model WebApp_building.DAL.Entities.Hizmetler
@{
    ViewData["Title"] = "AboutCreate";
    Layout = "~/Views/Layout/Index.cshtml";
}
<div class="main-panel">
    <div class="content">
        <div class="container-fluid">
            <h4 class="page-title">Hizmetler</h4>
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <div class="card-title">Hizmetler List</div>
                        </div>
                        <div class="card-body">

                            <form method="post">
                                <input type="hidden" asp-for="HizmetId" class="form-control" />
                                <label>Image</label>
                                <input type="text" asp-for="Image" class="form-control" />
                                <br />
                                <label>ImageTitle</label>
                                <input type="text" asp-for="ImageTitle" class="form-control" />
                                <br />
                                <label>ImageDescription</label>
                                <input type="text" asp-for="ImageDescription" class="form-control" />
                                <br />


                                <button class="btn btn-primary"> Hizmetler Add</button>
                            </form>


                        </div>
                    </div>

                </div>

            </div>
        </div>
    </div>
</div>

